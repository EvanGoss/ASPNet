@model IEnumerable<MyMusicStore.Models.Album>

@{
    ViewBag.Title = "Results";
}

<h2>Results</h2>

<table>
    <tr>
        <th>Artist</th>
        <th>Title</th>
        <th>Price</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Artist.Name</td>
            <td>@item.Title</td>
            <td>@String.Format("{0:c}",item.Price)</td>
        </tr>
    }
</table>